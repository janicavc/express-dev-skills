<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>
<body>
    <ul>
        <% skills.forEach(function(s) { %>
          <li>
            <%= s.skill %>
              - 
            <%= s.useful ? 'useful' : 'not useful' %>
          </li>
        <% }); %>
      </ul>
</body>
</html>